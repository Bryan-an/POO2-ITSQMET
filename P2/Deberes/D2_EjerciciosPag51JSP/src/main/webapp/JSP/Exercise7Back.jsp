        <title>Ejercicio 7</title>
    </head>
    <body>
        <%
            int sum = 0;

            for (int i = 7; i <= 100; i++) {
                if (i % 7 == 0) {
                    sum += i;
                }
            }

            out.println("<div class='container text-center py-5'>");
            out.println("<h3>");
            out.println("Sumatoria: " + sum);
            out.println("</h3>");
            out.println("</div>");
        %>
    </body>
